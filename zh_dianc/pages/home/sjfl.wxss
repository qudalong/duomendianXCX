.jzz {
    height: 80rpx;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    font-size: 30rpx;
    color: #666;
}

.jzz image {
    width: 60rpx;
    height: 60rpx;
    margin-right: 30rpx;
}

@-webkit-keyframes rotation {
    from {
        -webkit-transform: rotate(0deg);
    }

    to {
        -webkit-transform: rotate(360deg);
    }
}

.Rotation {
    -webkit-transform: rotate(360deg);
    animation: rotation 1s linear infinite;
}

.kong {
    position: fixed;
    top: 40%;
    left: 50%;
    transform: translate(-50%,-50%);
    height: auto;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 30rpx;
    color: #666;
}

.kong image {
    width: 200rpx;
    height: 200rpx;
    margin-bottom: 30rpx;
}

.selected {
    color: #000;
    font-weight: bold;
}

.selected .xsjt-img {
    transform: rotate(180deg);
}

.default {
    color: #666;
}

.paixu {
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    border-bottom: 1rpx solid #e5e5e5;
    margin-bottom: 30rpx;
}

.paixu>view {
    text-align: center;
    width: 25%;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    line-height: 70rpx;
    font-size: 26rpx;
    height: 70rpx;
}

.xsjt-img {
    width: 20rpx;
    height: 40rpx;
    margin-left: 20rpx;
    transition: transform .4s;
    transition: transform .4s,-webkit-transform .4s;
    transform: rotate(0deg);
}

.recommed {
    background-color: white;
    padding: 0rpx 20rpx;
}

.recommed_list {
    padding: 20rpx 0rpx;
    border-bottom: 1rpx solid #E7E7E8;
    display: flex;
}

.recommed_img {
    width: 100rpx;
    height: 100rpx;
    border-radius: 5rpx;
    background-color: green;
    margin-right: 15rpx;
}

.recommed_title {
    display: flex;
    margin-bottom: 10rpx;
    align-items: center;
}

.title_left {
    background-color: #FFDF32;
    font-size: 20rpx;
    height: 25rpx;
    width: 50rpx;
    padding: 3rpx 5rpx;
    border-radius: 5rpx;
    text-align: center;
    line-height: 25rpx;
    color: #333;
    margin-right: 10rpx;
}

.title_right {
    font-size: 24rpx;
    color: #333;
}

.pingjia_fenshu3 {
    position: relative;
    margin-right: 10rpx;
    width: 100rpx;
    height: 20rpx;
}

.pingfen {
    font-size: 30rpx;
    color: #f5622f;
    margin-right: 10rpx;
}

.kximg {
    position: absolute;
    width: 100rpx;
    height: 20rpx;
}

.kximg image {
    position: absolute;
    width: 100rpx;
    height: 20rpx;
}

.sximg {
    position: absolute;
    width: 100rpx;
    height: 20rpx;
    overflow: hidden;
}

.sximg image {
    position: absolute;
    width: 100rpx;
    height: 20rpx;
}

.recommed_midel {
    font-size: 24rpx;
    color: #666;
}

.recommed_end {
    font-size: 24rpx;
    color: #666;
    margin-top: 15rpx;
}

.recommed_end view {
    height: 25rpx;
    line-height: 20rpx;
}

.border_right {
    border-right: 1rpx solid #999;
    padding-right: 20rpx;
}

.border_right1 {
    padding: 0rpx 20rpx;
}

.border_right2 {
    padding: 0rpx 20rpx;
}

.recommed_right {
    font-size: 24rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-around;
    align-items: flex-end;
    margin-left: 25rpx;
}

.recommed_bao {
    color: #999;
    border: 1rpx solid #E7E7E8;
    width: 30rpx;
    height: 30rpx;
    text-align: center;
    line-height: 30rpx;
    border-radius: 5rpx;
}

.recommed_time {
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
}

.recommed_time view:nth-child(1) {
    color: #34AAFF;
    border: 1rpx solid #34AAFF;
    border-radius: 5rpx;
    padding: 0rpx 5rpx;
    width: 80rpx;
    text-align: center;
    line-height: 30rpx;
    height: 30rpx;
    margin-top: 7rpx;
}

.recommed_time view:nth-child(2) {
    background: #34AAFF;
    color: #fff;
    border: 1rpx solid #34AAFF;
    border-radius: 5rpx;
    padding: 0rpx 5rpx;
    width: 110rpx;
    text-align: center;
    line-height: 30rpx;
    height: 30rpx;
    margin-top: 7rpx;
    margin-left: 10rpx;
}

.recommed_mi {
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
}

.recommed_mi view:nth-child(1) {
    color: #8a8a8a;
    margin-top: 7rpx;
    margin-right: 10rpx;
}

.recommed_mi view:nth-child(2) {
    background: #999;
    width: 2rpx;
    height: 25rpx;
}

.recommed_mi view:nth-child(3) {
    color: #34AAFF;
    margin-top: 7rpx;
    margin-left: 10rpx;
}