    height: 20rpx;
}

.sximg {
    position: absolute;
    width: 100rpx;
    height: 20rpx;
    overflow: hidden;
}

.sximg image {
    position: absolute;
    width: 100rpx;
    height: 20rpx;
}

.recommed_midel {
    font-size: 24rpx;
    color: #666;
}

.recommed_end {
    font-size: 24rpx;
    color: #666;
    margin-top: 15rpx;
}

.recommed_end view {
    height: 25rpx;
    line-height: 20rpx;
}

.border_right {
    border-right: 1rpx solid #999;
    padding-right: 20rpx;
}

.border_right1 {
    padding: 0rpx 20rpx;
}

.border_right2 {
    padding: 0rpx 20rpx;
}

.recommed_right {
    font-size: 24rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-around;
    align-items: flex-end;
    margin-left: 25rpx;
}

.recommed_bao {
    color: #999;
    border: 1rpx solid #E7E7E8;
    width: 30rpx;
    height: 30rpx;
    text-align: center;
    line-height: 30rpx;
    border-radius: 5rpx;
}

.recommed_time {
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
}

.recommed_time view:nth-child(1) {
    color: #34AAFF;
    border: 1rpx solid #34AAFF;
    border-radius: 5rpx;
    padding: 0rpx 5rpx;
    width: 80rpx;
    text-align: center;
    line-height: 30rpx;
    height: 30rpx;
    margin-top: 7rpx;
}

.recommed_time view:nth-child(2) {
    background: #34AAFF;
    color: #fff;
    border: 1rpx solid #34AAFF;
    border-radius: 5rpx;
    padding: 0rpx 5rpx;
    width: 110rpx;
    text-align: center;
    line-height: 30rpx;
    height: 30rpx;
    margin-top: 7rpx;
    margin-left: 10rpx;
}

.recommed_mi {
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
}

.recommed_mi view:nth-child(1) {
    color: #8a8a8a;
    margin-top: 7rpx;
    margin-right: 10rpx;
}

.recommed_mi view:nth-child(2) {
    background: #999;
    width: 2rpx;
    height: 25rpx;
}

.recommed_mi view:nth-child(3) {
    color: #34AAFF;
    margin-top: 7rpx;
    margin-left: 10rpx;
}

.copyright {
    height: 120rpx;
    padding: 20rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    padding-top: 80rpx;
}

.copyright_logo {
    display: flex;
