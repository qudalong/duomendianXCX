.weui-search-bar {
    position: relative;
    padding: 10rpx 10rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    box-sizing: border-box;
    background-color: #EFEFF4;
    border-top: 0 solid #D7D6DC;
    border-bottom: 0 solid #D7D6DC;
    border-radius: 60px;
}

.weui-search-bar__form {
    border-radius: 60px;
}

.search-log {
    border: 1px solid #eee!important;
    display: inline-block!important;
    margin-left: 10px!important;
    margin-top: 8px!important;
    border-radius: 5px;
    padding-left: 14px;
    padding-right: 14px;
    box-sizing: border-box;
    text-align: center;
    text-decoration: none;
    line-height: 2;
}

.clear-search-log {
    width: 180rpx;
    height: 60rpx;
    line-height: 60rpx;
    background: #34AAFF;
    color: #fff;
    border-radius: 5px;
    margin: 10rpx auto;
    text-align: center;
}

.weui-search-bar__input {
    height: 80rpx;
    font-size: 30rpx;
}

.weui-search-bar__label {
    line-height: 80rpx;
    border-radius: 60px;
}

.weui-search-bar__box {
    line-height: 80rpx;
}

.weui-search-bar__cancel-btn {
    margin-left: 5px;
    margin-right: 5px;
    color: #34AAFF;
    font-size: 30rpx;
    line-height: 80rpx;
}

.weui-search-bar__text {
    font-size: 30rpx;
    line-height: 80rpx;
    vertical-align: top;
}

.jzz {
    height: 80rpx;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    font-size: 30rpx;
    color: #666;
}

.jzz image {
    width: 60rpx;
    height: 60rpx;
    margin-right: 30rpx;
}

@-webkit-keyframes rotation {
    from {
        -webkit-transform: rotate(0deg);
    }

    to {
        -webkit-transform: rotate(360deg);
    }
}

.Rotation {
    -webkit-transform: rotate(360deg);
    animation: rotation 1s linear infinite;
}

.kong {
    height: 80rpx;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    font-size: 26rpx;
    color: #666;
}

.kong image {
    width: 80rpx;
    height: 80rpx;
    margin-right: 30rpx;
}

.selected {
    color: #000;
    font-weight: bold;
}

.selected .xsjt-img {
    transform: rotate(180deg);
}

.default {
    color: #666;
}

.paixu {
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    border-bottom: 1rpx solid #e5e5e5;
    margin-bottom: 30rpx;
}

.paixu>view {
    text-align: center;
    width: 25%;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    line-height: 70rpx;
    font-size: 26rpx;
    height: 70rpx;
}

.xsjt-img {
    width: 20rpx;
    height: 40rpx;
    margin-left: 20rpx;
    transition: transform .4s;
    transition: transform .4s,-webkit-transform .4s;
    transform: rotate(0deg);
}

.recommed {
    margin-top: 15rpx;
    background-color: white;
    padding: 15rpx 30rpx;
}

.recommed_list {
    padding: 20rpx 0rpx;
    border-bottom: 1rpx solid #E7E7E8;
    display: flex;
}

.recommed_img {
    width: 100rpx;
    height: 100rpx;
    border-radius: 5rpx;
    background-color: green;
    margin-right: 15rpx;
}

.recommed_title {
    display: flex;
    margin-bottom: 10rpx;
    align-items: center;
}

.title_left {
    background-color: #FFDF32;
    font-size: 20rpx;
    height: 25rpx;
    width: 50rpx;
    padding: 3rpx 5rpx;
    border-radius: 5rpx;
    text-align: center;
    line-height: 25rpx;
    color: #333;
    margin-right: 10rpx;
}

.title_right {
    font-size: 26rpx;
    font-weight: bold;
    color: #333;
}

.pingjia_fenshu3 {
    position: relative;
    margin-right: 10rpx;
    width: 100rpx;
    height: 20rpx;
}

.pingfen {
    font-size: 30rpx;
    color: #f5622f;
    margin-right: 10rpx;
}

.kximg {
    position: absolute;
    width: 100rpx;
    height: 20rpx;
}

.kximg image {
    position: absolute;
