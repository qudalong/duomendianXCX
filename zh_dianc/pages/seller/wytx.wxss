page {
    background-color: #F5F5F5;
}

.head {
    width: 100%;
    box-sizing: border-box;
    background-color: #34aaff;
    padding: 20rpx 20rpx 30rpx;
}

.head .text {
    font-size: 30rpx;
    color: #fff;
    text-decoration: underline;
    text-align: right;
}

.head view:nth-child(2) {
    font-size: 50rpx;
    color: #fff;
    text-align: center;
}

.head view:nth-child(3) {
    font-size: 30rpx;
    color: #fff;
    text-align: center;
}

.weui-cell {
    padding: 20rpx 30rpx;
}

.weui-cell:before {
    left: 15px;
    right: 15px;
}

.weui-cells {
    font-size: 30rpx;
    margin-top: 20rpx;
    color: #666;
}

.weui-cell__bd {
    color: #666;
}

.weui-cell__ft {
    color: #333;
}

.weui-cells__title {
    font-size: 30rpx;
    padding: 20rpx 30rpx;
    background: #fff;
    color: #666;
    margin-top: 20rpx;
    margin-bottom: 0;
}

.list {
    margin-top: 20rpx;
    background-color: white;
    padding: 0rpx 30rpx;
    height: 90rpx;
}

.listimg {
    width: 40rpx;
    height: 40rpx;
    margin-right: 20rpx;
}

.trade {
    font-size: 28rpx;
    color: black;
}

.addright {
    width: 20rpx;
    height: 30rpx;
    margin-left: 20rpx;
}

.czhd {
    display: flex;
    flex: 1;
    flex-direction: row;
    align-items: center;
    font-size: 26rpx;
    color: #666;
}

.czhd view {
    line-height: 90rpx;
    text-align: right;
}

.czhd text {
    color: #34aaff;
    margin: 0 10rpx;
}

.czhd1 {
    background: #fff;
    padding: 0 30rpx 20rpx;
    font-size: 28rpx;
    color: #666;
}

.czhd1 text {
    color: #34aaff;
    margin: 0 10rpx;
}

.show {
    transform: rotate(90deg);
}

.txbd .weui-cell {
    padding: 5rpx 30rpx;
}

.txbd .weui-cells:after {
    border-bottom: none;
}

.read {
    display: flex;
    align-items: center;
    padding: 5rpx 30rpx;
    color: #999;
    font-size: 28rpx;
}

.readfont {
    color: #ff6161;
}

.kcrzxy {
    width: 70%;
    left: 50%;
    top: 15%;
    position: fixed;
    transform: translate(-50%,0);
    background: #fff;
    border-radius: 10px;
    z-index: 999;
}

.kcrzxyhd {
    text-align: center;
    font-size: 30rpx;
    background: #34aaff;
    height: 70rpx;
    line-height: 70rpx;
    color: white;
    border-top-left-radius: 10px;
    border-top-right-radius: 10px;
}

.kcrzxybd {
    font-size: 28rpx;
    padding: 20rpx 30rpx;
    box-sizing: border-box;
}

.queren {
    position: relative;
    height: 70rpx;
    width: 30%;
    left: 35%;
    font-size: 30rpx;
    line-height: 70rpx;
    text-align: center;
    color: white;
    background: #34aaff;
    border-radius: 10rpx;
    margin: 5px 0;
}

button:active {
    color: rgba(255,255,255,0.6);
    opacity: 0.6;
}

.zhezhao {
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    background: rgba(0,0,0,0.6);
    z-index: 998;
}

button[disabled][type="default"],button[disabled]:not([type]) {
    color: #fff;
}