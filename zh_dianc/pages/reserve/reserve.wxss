page {
    background-color: #F4F4F4;
}

.reserve {
    margin-top: 40rpx;
    background-color: white;
    border-top: 1rpx solid #E6E6E6;
    border-bottom: 1rpx solid #E6E6E6;
    font-size: 30rpx;
    margin-bottom: 40rpx;
}

.reserve_arrow {
    width: 20rpx;
    height: 30rpx;
    margin-left: 20rpx;
}

.reserve_date {
    display: flex;
    justify-content: center;
    align-items: center;
}

.reserve_date view {
    font-size: 30rpx;
    color: #333;
}

.reserve_type {
    height: 90rpx;
    border-bottom: 1rpx solid #E6E6E6;
    padding: 0rpx 30rpx;
}

.reserve_sele {
    color: #999;
    font-size: 28rpx;
    border: 1rp solid red;
}

.reserve_tab {
    text-align: right;
}

.reserve_man {
    display: flex;
    align-items: center;
}

.reserve_num {
    width: 300rpx;
}

.reserve_marks {
    font-size: 28rpx;
    background-color: white;
    padding: 20rpx 30rpx 30rpx;
}

.reserve_text {
    border: 1rpx solid #E6E6E6;
    width: 94%;
    height: 120rpx;
    padding: 20rpx;
    font-size: 28rpx;
    margin-top: 10rpx;
    border-radius: 6rpx;
}

.reserve_pay {
    font-size: 28rpx;
    color: #999;
    margin-top: 15rpx;
    text-indent: 2em;
}

.reserve_warn {
    display: flex;
    align-items: center;
}

.reserve_warn view {
    margin-left: 15rpx;
}

.reserve_money {
    color: #FF7272;
}

.footer {
    padding: 20rpx 30rpx;
    background-color: white;
    border-top: 1rpx solid #E6E6E6;
    border-bottom: 1rpx solid #E6E6E6;
    position: fixed;
    bottom: 0rpx;
    left: 0rpx;
    width: 92%;
}

.footer button {
    color: white;
    font-size: 36rpx;
    background-color: #34AAFF;
}

.drawer_screen {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 1000;
    background: #000;
    opacity: 0.5;
    overflow: hidden;
}

.drawer_box {
    width: 80vw;
    overflow: hidden;
    position: fixed;
    top: 13%;
    left: 0;
    z-index: 1001;
    background: #FFF;
    margin: 0 10vw 0 10vw;
    border-radius: 20rpx;
}

.zfimg {
    width: 50rpx;
    height: 45rpx;
    margin-right: 20rpx;
}

.zfimg image {
    width: 100%;
    height: 100%;
}

.yetext {
    color: #34AAFF;
    font-size: 26rpx;
    margin-left: 20rpx;
}

.zftext {
    font-size: 40rpx;
    margin-left: 20rpx;
}

.zfztext {
    font-size: 40rpx;
}

.radio-group .weui-cell {
    padding: 30rpx 30rpx;
    font-size: 30rpx;
}

.drawer_title {
    padding: 30rpx;
    font: 30rpx "microsoft yahei";
    font-weight: bold;
    text-align: center;
    color: #333;
    background: #F2F2F2;
}

.drawer_title1 {
    border-top: 0.5px solid #e6e6e6;
    padding: 20rpx;
    font: 27rpx "microsoft yahei";
    text-align: center;
    color: #999;
}

.drawer_content {
    height: 210px;
    overflow-y: scroll;
}

.btn_ok1 {
    padding: 25rpx;
    font: 30rpx "microsoft yahei";
    text-align: center;
    background: linear-gradient(to bottom right,#00CD66,#1aad19);
    color: #fff;
    border-radius: 0;
}

.btn_ok2 {
    padding: 25rpx;
    font: 30rpx "microsoft yahei";
    text-align: center;
    background: linear-gradient(to bottom right,#87CEFA,#34AAFF);
    color: #fff;
    border-radius: 0;
}

button[disabled][type="default"],button[disabled]:not([type]) {
    color: #fff;
    opacity: 0.8;
}